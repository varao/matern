@article{Woolley2010,
abstract = {In songbirds, species identity and developmental experience shape vocal behavior and behavioral responses to vocalizations. The interaction of species identity and developmental experience may also shape the coding properties of sensory neurons. We tested whether responses of auditory midbrain and forebrain neurons to songs differed between species and between groups of conspecific birds with different developmental exposure to song. We also compared responses of individual neurons to conspecific and heterospecific songs. Zebra and Bengalese finches that were raised and tutored by conspecific birds, and zebra finches that were cross-tutored by Bengalese finches were studied. Single-unit responses to zebra and Bengalese finch songs were recorded and analyzed by calculating mutual information (MI), response reliability, mean spike rate, fluctuations in time-varying spike rate, distributions of time-varying spike rates, and neural discrimination of individual songs. MI quantifies a response's capacity to encode information about a stimulus. In midbrain and forebrain neurons, MI was significantly higher in normal zebra finch neurons than in Bengalese finch and cross-tutored zebra finch neurons, but not between Bengalese finch and cross-tutored zebra finch neurons. Information rate differences were largely due to spike rate differences. MI did not differ between responses to conspecific and heterospecific songs. Therefore, neurons from normal zebra finches encoded more information about songs than did neurons from other birds, but conspecific and heterospecific songs were encoded equally. Neural discrimination of songs and MI were highly correlated. Results demonstrate that developmental exposure to vocalizations shapes the information coding properties of songbird auditory neurons.},
author = {Woolley, Sarah M N and Hauber, Mark E and Theunissen, Frederic E},
doi = {10.1002/dneu.20783},
file = {:nfs/home1/vrao/RSRCH/MJP/SVN/ctbn\_sampler/renewal/literature/birdsong\_gamma.pdf:pdf},
issn = {1932-846X},
journal = {Developmental neurobiology},
keywords = {Acoustic Stimulation,Action Potentials,Animals,Auditory Pathways,Auditory Pathways: growth \& development,Auditory Pathways: physiology,Auditory Perception,Auditory Perception: physiology,Finches,Housing, Animal,Information Theory,Male,Mesencephalon,Mesencephalon: growth \& development,Mesencephalon: physiology,Microelectrodes,Neurons,Neurons: physiology,Prosencephalon,Prosencephalon: growth \& development,Prosencephalon: physiology,Sound Spectrography,Species Specificity,Time Factors,Vocalization, Animal},
month = mar,
number = {4},
pages = {235--52},
pmid = {20039264},
title = {{Developmental experience alters information coding in auditory midbrain and forebrain neurons.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20039264},
volume = {70},
year = {2010}
}

